		<<link "slime">><<cheatsImpregPre>><<impregnate "slimes" 1000>><<cheatsImpreg true>><</link>> |
		<<link "Fish">><<cheatsImpregPre>><<impregnate "fish" 1000>><<cheatsImpreg true>><</link>> |
