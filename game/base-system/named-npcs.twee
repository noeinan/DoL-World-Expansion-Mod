<!-- Initialise NPC on first time called, introducing them to PC -->
<<if $NPCName[_npc].init isnot 1>>
	<<initnpc _npc>>
<</if>>

<!-- Set global variables -->
<<set $npc to _npc>>
<<set $NPCList[0].fullDescription to _npc>>

