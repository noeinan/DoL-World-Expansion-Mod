<<widget "npcrelationship">>
<!-- Robin, Avery, Kylar are unique and are treated differently -->
<<set _npc to $args[0]>>
<<if $NPCName[_npc].init is 1>>
	<<set $npctextlove to $NPCName[_npc].love>>
	<<set $npctextdom to $NPCName[_npc].dom>>
	<<set $npctextname to _npc>>
	<<set $npctextdescription to $NPCName[_npc].title>>
	<<relationshiptext _npc>>
	<br>
<</if>>
<</widget>>

<<widget "npcincr">>
	<!-- Arguments passed are NNPC name, variable type, and value (+/-)-->

	<<set _npc to $args[0]>>
	<<set _var to $args[1]>>
	<<set _val to $args[2]>>

	<<switch _var>>
	<<case love>>
		<<set $NPCName[_npc].love += _val>>
	<<case dom>>
		<<set $NPCName[_npc].dom += _val>>
	<<case trust>>
		<<set $NPCName[_npc].trust += _val>>
	<<case lust>>
		<<set $NPCName[_npc].lust += _val>>
	<<case rage>>
		<<set $NPCName[_npc].rage += _val>>
	<<case state>>
		<<set $NPCName[_npc].state to _val>>
	<<case init>>
		<<set $NPCName[_npc].init to _val>>
	<<case trauma>>
		<<set $NPCName[_npc].trauma += _val>>
	<<default>>
		<span class="red">NPCINCR error - Unknown variable _var</span>
		<br><br>
	<</switch>>

<</widget>>

<<widget "npcset">>
<!-- Sets selected NNPC variables using NNPC name-->
<!-- Arguements passed are NNPC name, variable type, value -->

<<set _npc to $args[0]>>
<<set _var to $args[1]>>
<<set _val to $args[2]>>

<<switch _var>>
	<<case love>>
		<<set $NPCName[_npc].love to _val>>
	<<case dom>>
		<<set $NPCName[_npc].dom to _val>>
	<<case trust>>
		<<set $NPCName[_npc].trust to _val>>
	<<case lust>>
		<<set $NPCName[_npc].lust to _val>>
	<<case rage>>
		<<set $NPCName[_npc].rage to _val>>
	<<case state>>
		<<set $NPCName[_npc].state to _val>>
	<<case init>>
		<<set $NPCName[_npc].init to _val>>
	<<case trauma>>
		<<set $NPCName[_npc].trauma to _val>>
	<<default>>
		<span class="red">NPCSET error - Unknown value _var</span>
		<br><br>
<</switch>>

<</widget>>

